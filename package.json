{
  "name": "alchemy-client",
  "version": "0.6.0",
  "description": "React based client app for interacting with DAO Stack",
  "author": "Tibet Sprague",
  "license": "GPL-3.0",
  "main": "./src/index.tsx",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daostack/alchemy.git"
  },
  "engines": {
    "node": "=9.5",
    "npm": "=5.10"
  },
  "jest": {
    "testURL": "http://127.0.0.1:3000/",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/",
      "/test/integration/"
    ],
    "testRegex": "(/test/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(scss|css|less)$": "identity-obj-proxy",
      "^arc": "<rootDir>/src/arc",
      "^actions(.*)$": "<rootDir>/src/actions$1",
      "^components(.*)$": "<rootDir>/src/components$1",
      "^constants(.*)$": "<rootDir>/src/constants$1",
      "^layouts(.*)$": "<rootDir>/src/layouts$1",
      "^lib(.*)$": "<rootDir>/src/lib$1",
      "^reducers(.*)$": "<rootDir>/src/reducers$1",
      "^selectors(.*)$": "<rootDir>/src/selectors$1",
      "^schemas(.*)$": "<rootDir>/src/schemas$1",
      "^src(.*)$": "<rootDir>/src$1"
    }
  },
  "scripts": {
    "analyze": "NODE_ENV=production ANALYZE=1 node --max_old_space_size=4096 node_modules/.bin/webpack --config webpack.prod.config.js --progress --colors",
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.config.js",
    "lint": "tslint --project .",
    "populate-dao": "ts-node scripts/populateDAO.ts",
    "setup-env": "npm run subgraph:deploy && node scripts/getmigrationfile.js",
    "service-status": "node scripts/serviceStatus.js",
    "subgraph:deploy": "npm explore @daostack/subgraph -- npm install && npm explore @daostack/subgraph -- npm run deploy",
    "start": "webpack-dev-server --config webpack.dev.config.js",
    "start-dev-profiling": "cross-env NODE_ENV=dev-profiling API_URL=https://daostack-alchemy.herokuapp.com S3_BUCKET=daostack-alchemy-staging webpack-dev-server --config webpack.dev.config.js",
    "start-docker": "webpack-dev-server --config webpack.docker.config.js",
    "start-prod": "cross-env NODE_ENV=production webpack-dev-server --config webpack.prod.config.js",
    "start-staging": "cross-env S3_BUCKET=daostack-alchemy-staging webpack-dev-server --config webpack.dev.config.js",
    "test": "npm run test:integration",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:integration": "wdio ./test/integration/wdio.conf.js",
    "test:integration:headless": "wdio ./test/integration/wdio-headless.conf.js"
  },
  "dependencies": {
    "@daostack/arc.js": "0.0.0-alpha.101",
    "@daostack/client": "0.0.11",
    "@fortawesome/fontawesome-svg-core": "^1.2.10",
    "@fortawesome/free-brands-svg-icons": "^5.6.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@types/graphql": "^14.0.4",
    "aws-sdk": "^2.380.0",
    "axios": "^0.17.1",
    "bignumber.js": "^5.0.0",
    "bip39": "^2.2.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.7",
    "disqus-react": "^1.0.5",
    "dotenv": "^6.2.0",
    "es6-promisify": "^5.0.0",
    "eth-sig-util": "^2.1.0",
    "ethereum-blockies-png": "^0.1.3",
    "ethereumjs-util": "^6.0.0",
    "ethereumjs-wallet": "^0.6.3",
    "ethers": "^2.1.2",
    "ethjs-abi": "0.1.8",
    "express": "^4.16.4",
    "formik": "^0.11.11",
    "heroku-client": "^3.0.7",
    "immutability-helper": "2.6.4",
    "intro.js": "^2.9.3",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "normalizr": "^3.3.0",
    "oauthio-web": "^0.6.2",
    "object-hash": "^1.3.1",
    "query-string": "^6.2.0",
    "rc-tooltip": "^3.7.3",
    "react": "^16.7.0",
    "react-cookie": "^2.1.4",
    "react-dom": "^16.7.0",
    "react-icons": "^3.2.2",
    "react-joyride": "^2.0.0",
    "react-linkify": "^0.2.2",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.2.2",
    "react-router-modal": "^1.5.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-share": "^2.4.0",
    "react-transition-group": "^2.5.1",
    "redis": "^2.8.0",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.7",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.9.1",
    "redux-promise-middleware": "^4.4.1",
    "redux-thunk": "2.2.0",
    "reselect": "^3.0.1",
    "run-with-ganache": "^0.1.1",
    "socket.io-client": "^2.2.0",
    "style-loader": "^0.18.2",
    "truffle-contract": "^3.0.1",
    "ts-node": "^5.0.1",
    "web3-provider-engine": "^14.1.0",
    "webdriverio": "^4.14.1",
    "webpack": "^3.11.0",
    "ws": "^6.1.2",
    "zeppelin-solidity": "^1.12.0"
  },
  "devDependencies": {
    "@daostack/migration": "0.0.0-alpha.58-v9",
    "@daostack/subgraph": "git+https://github.com/daostack/subgraph.git",
    "@types/classnames": "^2.2.6",
    "@types/es6-promisify": "^5.0.0",
    "@types/ethereumjs-util": "^5.2.0",
    "@types/jest": "^22.2.0",
    "@types/lodash": "^4.14.119",
    "@types/node": "^8.10.39",
    "@types/object-hash": "^1.2.0",
    "@types/prop-types": "^15.5.8",
    "@types/query-string": "^6.1.1",
    "@types/rc-tooltip": "^3.7.0",
    "@types/react": "16.4.6",
    "@types/react-dom": "16.0.6",
    "@types/react-fontawesome": "^1.6.4",
    "@types/react-hot-loader": "^3.0.4",
    "@types/react-redux": "^5.0.21",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-router-redux": "^5.0.17",
    "@types/react-share": "^2.1.1",
    "@types/react-transition-group": "^2.0.15",
    "@types/redis": "^2.8.8",
    "@types/redux-logger": "^3.0.3",
    "@types/redux-promise-middleware": "^0.0.8",
    "@types/socket.io-client": "^1.4.32",
    "@types/webpack-env": "^1.13.0",
    "@types/yargs": "^11.1.1",
    "awesome-typescript-loader": "^3.2.3",
    "axios-mock-adapter": "^1.15.0",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.1.3",
    "eslint": "^5.10.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "ganache-cli": "^6.2.3",
    "html-webpack-plugin": "^3.0.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.2",
    "node-sass": "^4.11.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "sass-loader": "^6.0.6",
    "sass-resources-loader": "^1.3.5",
    "source-map-loader": "^0.2.4",
    "spawn-command": "0.0.2-1",
    "ts-jest": "^22.4.1",
    "tslint": "^5.12.0",
    "tslint-react": "^3.5.1",
    "typescript": "^2.9",
    "typings-for-css-modules-loader": "^1.5.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "wdio-mocha-framework": "^0.6.4",
    "wdio-selenium-standalone-service": "0.0.11",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-dev-server": "^2.11.3",
    "webpack-merge": "^4.1.5",
    "winston": "^3.1.0",
    "yargs": "^12.0.5"
  }
}
